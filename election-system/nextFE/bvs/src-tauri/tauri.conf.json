{
  "productName": "BVS",
  "version": "0.31.2",
  "identifier": "com.goettertz.bvs",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3002",
    "frontendDist": "../out"
  },
  "app": {
    "withGlobalTauri": true,    
    "windows": [
      {
        "title": "BVS",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}